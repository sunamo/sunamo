    <None Remove="Converters\**" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="Plugin.NetStandardStorage" Version="1.0.7" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Helpers\Runtime\" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="HtmlAgilityPack">
      <HintPath>..\dll\HtmlAgilityPack.dll</HintPath>
    </Reference>
    <Reference Include="win">
      <HintPath>..\win\bin\Release\win.dll</HintPath>
    </Reference>
    <Reference Include="YamlDotNet">
      <HintPath>..\dll\YamlDotNet.dll</HintPath>
    </Reference>
  </ItemGroup>
