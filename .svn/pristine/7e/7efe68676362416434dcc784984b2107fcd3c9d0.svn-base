using sunamo;
using desktop.Essential;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Media;

namespace desktop
{
    public class LogMessage : LogMessageAbstract<Color, string>//, ILogMessage
    {
        public LogMessage()
        {

        }

        protected async override void SetBg(Color c)
        {
            await ThisApp.cd.InvokeAsync(() => {
                Bg = c;
            }, ThisApp.cdp);
        }
    }
}
